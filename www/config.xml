<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "eu.edeldruk.sampleSelector"
    version   = "1.0.0">

	<name>-edeldruk-</name>
	
	<description>
	   sample selector
	</description>
	
	<author href="http://www.bobstoute.nl" email="stoute.bob@gmail.com">Bob Stoute</author>
	
	<icon src="assets/icons/app-icon-512.png" width="512" height="512" />
	<icon src="assets/icons/app-icon-114.png" width="96"  height="96"  gap:platform="android" />
	<icon src="assets/icons/app-icon-114.png" width="80"  height="80"  gap:platform="blackberry" />
	<icon src="assets/icons/app-icon-128.png" width="144" height="144" gap:platform="ios" />
	
	<preference name="phonegap-version" value="2.2.0" />
	<preference name="target-device" value="tablet" />
	<preference name="fullscreen" value="true" />
	<preference name="orientation" value="landscape" />
	<preference name="stay-in-webview" value="true" />
	<preference name="show-splash-screen-spinner" value="true" />
	<preference name="prerendered-icon" value="true" />

	<preference name="permissions" value="none" />
	
	<access origin="*" />
	
	<gap:splash src="assets/images/splash.png" />

